#include <iostream>
#include <iomanip>

int main() {
    int a = 7, b = 3;
    float x = 7.0f, y = 3.0f;
    double m = 7.0, n = 3.0;

    std::cout << "Operações com int:" << std::endl;
    std::cout << "7 / 3 = " << a / b << std::endl;

    std::cout << "\nOperações com float:" << std::endl;
    std::cout << "7.0f / 3.0f = " << x / y << std::endl;

    std::cout << "\nOperações com double:" << std::endl;
    std::cout << std::setprecision(15);
    std::cout << "7.0 / 3.0 = " << m / n << std::endl;

    return 0;
}
