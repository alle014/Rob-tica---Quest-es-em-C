// Example program
#include <iostream>
#include <string>
using namespace std;

void troca(int &a, int &b){
    int temporario = a;
    a = b;
    b = temporario;
}

int main(){
    int x;
    int y;
    
    cout << "digite um numero para x: " << endl;
    cin >> x;
    cout << "digite um numero para y: " << endl;
    cin >> y;
    
    troca(x, y);
    
    cout << "valor pos troca: x = " << x << " y = " << y << endl; 
}
