// Example program
#include <iostream>
#include <string>
using namespace std;

int soma(int a, int b){
    return a + b;
}

int main(){
    int numero1, numero2;
    
    std::cout << "Digite o primeiro numero: " << std::endl;
    cin >> numero1;

    std::cout << "Digite o segundo numero: " << std::endl;
    cin >> numero2;
    
    int resultado = soma(numero1, numero2);
    std::cout << "A soma e igual a: " << resultado << endl;
}
