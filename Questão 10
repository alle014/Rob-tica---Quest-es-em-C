// Example program
#include <iostream>
#include <string>
using namespace std;

float maiorque (float numero1, float numero2){
    if (numero1 > numero2){
        std::cout << numero1 << std::endl;
    } else if (numero2 > numero1){
        std::cout << numero2 << std::endl;
    } else {
        std::cout << "Os numeros sao iguais." << std::endl;
    }
}

int main(){
    float nmr1;
    float nmr2;
    
    std::cout << "Insira um numero decimal qualquer: " << std::endl;
    cin >> nmr1;
    std::cout << "Insira outro numero decima qualquer: " << std::endl;
    cin >> nmr2;
    
    float resultado = maiorque (nmr1, nmr2);
    
    std::cout << resultado << std::endl;
}
