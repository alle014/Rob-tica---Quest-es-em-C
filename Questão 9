// Example program
#include <iostream>
#include <string>
using namespace std;

int quadrado(int a){
    return a*a;
}

int main(){
    int numero;
    
    std::cout << "Digite um numero inteiro qualquer: " << std::endl;
    cin >> numero;
    
    int resultado = quadrado(numero);
    std::cout << "O quadrado do numero digitado e: " << resultado << std::endl;
}
